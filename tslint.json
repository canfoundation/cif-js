{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "eofline": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "ordered-imports": false,
    "no-submodule-imports": false,
    "no-var-requires": false,
    "no-empty": false,
    "variable-name": false,
    "no-bitwise": false,
    "ban": [
      true,
      "eval",
      { "name": "fdescribe", "message": "don't focus test suite" },
      { "name": ["describe", "only"], "message": "don't focus test suite" },
      { "name": "fit", "message": "don't focus test case" },
      { "name": ["it", "only"], "message": "don't focus test case" }
    ],
    "no-duplicate-imports": true
  }
}
